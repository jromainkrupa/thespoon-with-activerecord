<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>

<% @restaurants.each do |restaurant| %>
  </div>
    <h3><%= restaurant.name %></h3>
    <p> <%= restaurant.address %></p>
    <h6><%= restaurant.ratings %></h6>
    <%= link_to "see more", restaurant_path(restaurant)  %>
<%= link_to "Delete #{restaurant.name}",
            restaurant_path(restaurant),
            method: :delete,
            data: { confirm: "Are you sure?" } %>  <div>
<% end %>